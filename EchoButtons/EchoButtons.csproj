    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Zaron Thompson</Authors>
    <Company>Zaron Thompson</Company>
    <Product>Echo Buttons</Product>
    <Description>C# API for Echo Buttons paired on Windows</Description>
